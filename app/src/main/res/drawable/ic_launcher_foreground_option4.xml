<?xml version="1.0" encoding="utf-8"?>
<!-- OPTION 4: Stacked Cards "F" - Credit card/wallet theme -->
<!-- F emerging from stacked card shapes, fintech modern -->
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">

    <!-- Back card (darker) -->
    <path
        android:pathData="M22,36 Q22,30 28,30 L80,30 Q86,30 86,36 L86,66 Q86,72 80,72 L28,72 Q22,72 22,66 Z"
        android:fillColor="#3D3D5C"/>

    <!-- Front card with gradient -->
    <path
        android:pathData="M18,42 Q18,36 24,36 L76,36 Q82,36 82,42 L82,72 Q82,78 76,78 L24,78 Q18,78 18,72 Z">
        <aapt:attr name="android:fillColor">
            <gradient
                android:startX="18"
                android:startY="36"
                android:endX="82"
                android:endY="78"
                android:type="linear">
                <item android:offset="0" android:color="#FF667EEA"/>
                <item android:offset="1" android:color="#FF764BA2"/>
            </gradient>
        </aapt:attr>
    </path>

    <!-- "F" on card -->
    <path
        android:pathData="M32,44 L58,44 L58,50 L40,50 L40,56 L54,56 L54,62 L40,62 L40,72 L32,72 Z"
        android:fillColor="#FFFFFF"/>

    <!-- Card chip (gold accent) -->
    <path
        android:pathData="M62,52 Q62,50 64,50 L72,50 Q74,50 74,52 L74,60 Q74,62 72,62 L64,62 Q62,62 62,60 Z"
        android:fillColor="#FFD700"/>
</vector>
